{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:PutObjectRetention",
                "s3:PutObjectLegalHold",
                "s3:GetObjectVersion",
                "s3:CopyObject",
                "s3:ListBucket",
                "s3:HeadObject"
            ],
            "Resource": "*"
        }
    ]
}